{
  "name": "memcube-testing",
  "version": "1.0.0",
  "description": "Comprehensive testing framework for MemCube system efficacy",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:performance": "node --loader tsx tests/performance/benchmark.ts",
    "generate:data": "tsx scripts/generate-test-data.ts",
    "analyze:efficacy": "tsx scripts/analyze-efficacy.ts",
    "dashboard": "vite"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "openai": "^4.24.0",
    "zod": "^3.22.4",
    "date-fns": "^3.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.10.4",
    "@tanstack/react-query": "^5.17.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  }
}